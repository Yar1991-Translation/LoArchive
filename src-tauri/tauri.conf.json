{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "LoArchive",
  "version": "1.0.0",
  "identifier": "com.loarchive.app",
  "build": {
    "frontendDist": "../templates",
    "devUrl": "http://localhost:5000",
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "LoArchive",
        "label": "main",
        "width": 1200,
        "height": 750,
        "minWidth": 900,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "center": true,
        "decorations": false,
        "transparent": false,
        "shadow": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.ico",
      "icons/icon.png"
    ],
    "resources": [
      "../web_app.py",
      "../login_info.py",
      "../login_info.example.py",
      "../requirements.txt",
      "../l*.py",
      "../tool.py",
      "../parse_template.py",
      "../useragentutil.py"
    ],
    "windows": {
      "webviewInstallMode": {
        "type": "embedBootstrapper"
      }
    }
  },
  "plugins": {}
}
